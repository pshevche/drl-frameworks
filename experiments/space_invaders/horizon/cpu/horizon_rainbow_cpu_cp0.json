{
    "env": "SpaceInvaders-v0",
    "model_type": "pytorch_discrete_dqn",
    "max_replay_memory_size": 1000000,
    "use_gpu": false,
    "rl": {
        "gamma": 0.99,
        "target_update_rate": 0.032,
        "maxq_learning": 1,
        "epsilon": 0.00015,
        "temperature": 0.35,
        "softmax_policy": 0
    },
    "rainbow": {
        "double_q_learning": true,
        "dueling_architecture": true
    },
    "training": {
        "layers": [
            -1,
            128,
            64,
            -1
        ],
        "activations": [
            "relu",
            "relu",
            "linear"
        ],
        "minibatch_size": 32,
        "learning_rate": 0.0000625,
        "optimizer": "ADAM",
        "lr_decay": 0.999,
        "cnn_parameters": {
            "conv_dims": [
                3,
                32,
                16
            ],
            "conv_height_kernels": [
                8,
                4
            ],
            "conv_width_kernels": [
                8,
                4
            ],
            "pool_kernels_strides": [
                2,
                2
            ],
            "pool_types": [
                "max",
                "max"
            ]
        }
    },
    "run_details": {
        "num_episodes": 1e9,
        "max_steps": 27000,
        "train_every_ts": 1,
        "train_after_ts": 1,
        "test_every_ts": 1000,
        "test_after_ts": 1,
        "num_train_batches": 1,
        "avg_over_num_episodes": 100,
        "avg_over_num_steps": 125000,
        "offline_train_epochs": 30,
        "timesteps_total": 5e7,
        "checkpoint_after_ts": 0
    }
}